{
  "name": "@cajacko/cz-conventional-changelog",
  "version": "0.1.0",
  "description": "Commitizen adapter following the conventional-changelog format.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "mocha *.test.js",
    "format": "prettier --write *.js",
    "semantic-release": "semantic-release",
    "publish": "npm publish --access public"
  },
  "homepage": "https://github.com/commitizen/cz-conventional-changelog",
  "repository": {
    "type": "git",
    "url": "https://github.com/commitizen/cz-conventional-changelog.git"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 10"
  },
  "author": "Jim Cummins <jimthedev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@cajacko/template": "^1.5.3",
    "@commitlint/lint": "^8.1.0",
    "chalk": "^2.4.1",
    "commitizen": "^4.0.3",
    "conventional-commit-types": "^2.0.0",
    "fs-extra": "^8.1.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "lodash.map": "^4.5.1",
    "longest": "^2.0.1",
    "punycode": "^2.1.1",
    "right-pad": "^1.0.1",
    "unicode-emoji-annotations": "^1.0.4",
    "word-wrap": "^1.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cosmiconfig": "^5.2.1",
    "mocha": "^6.2.0",
    "mock-require": "^3.0.3",
    "prettier": "^1.15.3",
    "semantic-release": "^15.13.3",
    "semver": "^6.2.0"
  },
  "optionalDependencies": {
    "@commitlint/load": ">6.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./index.js"
    }
